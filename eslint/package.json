{
  "scripts": {
    "eslint": "eslint"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:react/recommended"
    ],
    "env": {
      "node": true,
      "jest": true,
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "import/no-unresolved": "off",
      "react/jsx-uses-vars": "error"
    }
  },
  "dependencies": {
    "babel-eslint": "*",
    "eslint": "*",
    "eslint-formatter-markdown": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.7.0"
  }
}
