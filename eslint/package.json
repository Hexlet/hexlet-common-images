{
  "scripts": {
    "eslint": "eslint"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "node": true,
      "jest": true,
      "browser": true
    },
    "rules": {
      "import/no-unresolved": "off"
    }
  },
  "dependencies": {
    "babel-eslint": "*",
    "eslint": "*",
    "eslint-formatter-markdown": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*"
  }
}
